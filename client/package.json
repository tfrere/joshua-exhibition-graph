{
  "name": "my-r3f-graph",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build:wasm": "emcc src/wasm/force-calculations.cpp -o public/wasm/force-calculations.js --bind -s WASM=1 -O3 -s ALLOW_MEMORY_GROWTH=1 -s \"EXPORTED_RUNTIME_METHODS=['getValue','setValue','ccall','cwrap']\" -s EXPORTED_FUNCTIONS=['_malloc','_free'] -s WASM_BIGINT=1 -s ASSERTIONS=1 -s MODULARIZE=1 -s EXPORT_NAME=createModule -s NO_DYNAMIC_EXECUTION=1 -s ENVIRONMENT=web -s SINGLE_FILE=1 -std=c++11",
    "dev": "vite",
    "build": "yarn build:wasm && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "serve -s dist --config serve.json"
  },
  "dependencies": {
    "@react-spring/three": "^9.7.5",
    "@react-three/drei": "^9.121.5",
    "@react-three/fiber": "^8.17.14",
    "@react-three/postprocessing": "^2.19.1",
    "@types/d3": "^7.4.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/three": "^0.173.0",
    "d3": "^7.9.0",
    "gsap": "^3.12.7",
    "r3f-forcegraph": "^1.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "serve": "^14.2.1",
    "socket.io-client": "^4.7.4",
    "three": "^0.173.0",
    "tweakpane": "^4.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.2.0",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
